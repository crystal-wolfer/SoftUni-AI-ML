def encode(text):
    """
    Returns the run-length encoded version of the text
    (numbers after symbols, length = 1 is skipped)
    """
    if len(text) == 1:
        return "Input length is 1 - nothing to encode"

    result = ""
    count = 1
    i = 0
    
    while i < len(text):
        if i + 1 < len(text)  and text[i] == text[i+1]:
            count += 1
        else:
            if count == 1:
                result += text[i]
            else:
                result += text[i] + str(count)
                count = 1
        i += 1

    return result


   encode("AAAGTCTGACAGTTACCGTTAA")


def decode(text):
    if len(text) == 1:
        return "Input length is 1 - nothing to encode"

    result = ""
    i = 0

    while i < len(text):
        if text[i].isalpha():
            #checking next char if is a number
            if i + 1 < len(text) and text[i+1].isdigit():
                result += text[i] * int(text[i+1]) #repeat char n-times
                i += 2
            else:
                result += text[i]
                i += 1
        i += 0

    return (result)      
    


