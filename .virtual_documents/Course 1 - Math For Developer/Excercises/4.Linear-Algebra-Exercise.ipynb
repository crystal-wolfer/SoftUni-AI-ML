def plot_vectors(vectors, colors):
    """
    Plots vectors on the xy-plane. The `vectors` parameter is a Python list.
    Each vector is specified in the format [start_x, start_y, end_x, end_y]
    """
    X, Y, end_x, end_y = zip(*vectors)
    #Since zip() returns tuples which are imutables, I need to convert to numpy arrays to be able to compute U and V which define the arrow direction
    X, Y, end_x, end_y = np.array(X), np.array(Y), np.array(end_x), np.array(end_y)
    
    U = end_x - X
    V = end_y - Y

    #Plotting the quiver() function and plotting the results
    plt.quiver(X, Y, U, V, angles = "xy", scale_units = "xy", scale = 1, color = colors)
    plt.xlim(-5, 5)
    plt.ylim(-5, 5)
    plt.show


plot_vectors([[0, 0, 2, 3]], ["red"]) # One vector
plot_vectors([[0, 0, 1, 0], [0, 0, 0, 1]], ["red", "blue"]) # Two orthogonal vectors





