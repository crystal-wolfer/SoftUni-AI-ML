def interpolate_polynomial(points, degree, min_x, max_x):
    """
    Interpolates a polynomial of the specified degree through the given points and plots it
    points - a list of points (x, y) to plot
    degree - the polynomial degree
    min_x, max_x - range of x values used to plot the interpolating polynomial
    """
    x_points = points[:,0]
    y_points = points[:,1]
    p_func = poly.Polynomial.fit(x_points, y_points, degree)

    x_func = np.linspace(min_x, max_x, 50)
    y_func = p_func(x_func)

    #Plotting the given points and the polynomial function
    plt.scatter(x_points,y_points, c = "r")
    plt.plot(x_func, y_func, c = "g") 
    plt.xlabel("x")
    plt.ylabel("y")
    plt.show()



