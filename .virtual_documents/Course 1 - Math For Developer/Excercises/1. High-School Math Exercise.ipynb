import sympy
import math

















x = sympy.symbols("x")
a, b, c = sympy.symbols("a b c")

#Solving in general
res = sympy.solve(a*x**2 + b*x + c)
print(res)





#sympy has chosen to solve for a which is not what I am looking for. I need sympy to solve this equation for x:
res = sympy.solve(a*x**2 + b*x + c, x)
print(res)





#* Optional: If you want, you can write your own Python function which calculates the roots.

def solve(a, b, c):
    d = b**2 - 4*a*c

    if d<0:
        return "No real roots"
    else:
        sqrt_d = math.sqrt(d)
        x1 = (-b - sqrt_d)/2*a
        x2 = (-b + sqrt_d)/2*a
        return x1, x2


solve(1, 2, 3)


solve(-4,6,-2)


solve(1, 0, 0)





